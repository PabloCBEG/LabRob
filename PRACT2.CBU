  SPEED     25
  MOVED     INT2 
  MOVELD    INT 
  OPEN      
  MOVELD    INT2 
  SPEED     70
  MOVED     REST 
  WAIT      IN[1] = 1
  MOVED     INT2 
  SPEED     25
  MOVELD    INT 
  CLOSE     
  MOVELD    INT2 
  SPEED     70
  MOVED     AUX3 
